{
  "name": "markdown-notebook",
  "private": true,
  "version": "0.1.0",
  "repository": "git@github.com:marvindv/markdown-notebook.git",
  "author": "Marvin Davieds <marvin.davieds@gmail.com>",
  "license": "AGPL-3.0",
  "dependencies": {
    "husky": "^4.2.5",
    "lint-staged": "^10.1.5",
    "prettier": "^2.0.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "packages/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  }
}
